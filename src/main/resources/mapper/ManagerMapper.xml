<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.go.mapper.ManagerMapper">

    <select id="getManagerByUsername" resultType="com.example.go.entity.Manager">
        select *
        from manager
        where username = #{username}
    </select>
</mapper>